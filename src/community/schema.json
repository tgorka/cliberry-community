{
  "$schema": "http://json-schema.org/schema",
  "id": "cliberry-schematics#community",
  "title": "cliberry-schematics community Options Schema",
  "type": "object",
  "properties": {
    "code-of-conduct": {
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": "0"
      },
      "alias": "coc",
      "description": "Do generate code of conduct. By default true."
    },
    "email": {
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": "1"
      },
      "alias": "e",
      "description": "Con\ntact email for code of conduct. Empty by default."
    },
    "contributing": {
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": "2"
      },
      "alias": "c",
      "description": "Do generate contributing. By default true."
    },
    "pull-request": {
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": "3"
      },
      "alias": "pr",
      "description": "Do generate pull request template. By default tru\ne"
    },
    "bug-report": {
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": "4"
      },
      "alias": "br",
      "description": "Do generate bug report template. By default true."
    },
    "feature-request": {
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": "5"
      },
      "alias": "fr",
      "description": "Do generate feature request. By default true"
    },
    "custom": {
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": "6"
      },
      "alias": "cm",
      "description": "Do generate custm t\nemplate. By default true"
    }
  },
  "required": [
    "code-of-conduct",
    "email",
    "contributing",
    "pull-request",
    "bug-report",
    "feature-request",
    "custom"
  ]
}
