{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "init": {
      "aliases": [
        "ng-new",
        "new"
      ],
      "description": "Init schematics collection for cliberry-doc.",
      "factory": "./init/index#init",
      "schema": "./init/schema.json"
    },
    "license": {
      "factory": "./license/index#license",
      "schema": "./license/schema.json",
      "description": "Generate license of the project",
      "aliases": [
        "l"
      ]
    },
    "community": {
      "factory": "./community/index#community",
      "schema": "./community/schema.json",
      "description": "Generate community files of the project",
      "aliases": [
        "c"
      ]
    },
    "readme": {
      "factory": "./readme/index#readme",
      "schema": "./readme/schema.json",
      "description": "Generate readme and the pages related to the documentation",
      "aliases": [
        "r"
      ]
    }
  }
}